extends layout

//- block hero-unit
//-     .jumbotron
block header
    header
        .container
            .intro-text
                .intro-lead-in Start a new trip with LunaBnB
                .intro-heading Welcome To 
                  span.brand_name LunaBnB!
                a.page-scroll.btn.btn-xl(href='#services') 
                 | Login
// Services Section
block services
    section#services 
      .container
        .row
          .col-lg-12.text-center
            h2.section-heading Services
            hr 
            br
        .row.text-center
          .col-md-4
            span.fa-stack.fa-4x
              i.fa.fa-circle.fa-stack-2x.text-primary
              i.fa.ion-briefcase.fa-stack-1x.fa-inverse
            h4.service-heading Tourist
            p.text-muted 
              | 루나비앤비와 함께 새로운 여행을 시작하세요
          .col-md-4
              span.fa-stack.fa-4x
                  i.fa.fa-circle.fa-stack-2x.text-primary
                  i.fa.ion-android-contact.fa-stack-1x.fa-inverse
              h4.service-heading Host
              p.text-muted
                  | 루나비앤비의 호스트가 되어 사람들을 환영해주고 이들이 집처럼 편안함을 느낄 수 있도록 도와주세요.
          .col-md-4
              span.fa-stack.fa-4x
                  i.fa.fa-circle.fa-stack-2x.text-primary
                  i.fa.ion-iphone.fa-stack-1x.fa-inverse
              h4.service-heading Responsive Design
              p.text-muted
                  | 모바일 기기로도 쉽게 이용하세요.
    // Portfolio Grid Section
  block portfolio
    section#portfolio.bg-light-gray
        .container
            .row
                .col-lg-12.text-center
                    h2.section-heading Portfolio
                    h3.section-subheading.text-muted Lorem ipsum dolor sit amet consectetur.
            .row
                .col-md-4.col-sm-6.portfolio-item
                    a.portfolio-link(href='#portfolioModal1', data-toggle='modal')
                        .portfolio-hover
                            .portfolio-hover-content
                              i.fa.ion-search.fa-4x
                        img.img-responsive(src='/images/header_bg.jpg', alt='')
                    .portfolio-caption
                        h4 Round Icons
                        p.text-muted Graphic Design
                .col-md-4.col-sm-6.portfolio-item
                    a.portfolio-link(href='#portfolioModal2', data-toggle='modal')
                        .portfolio-hover
                            .portfolio-hover-content
                              i.fa.ion-search.fa-4x
                        img.img-responsive(src='/images/header_bg.jpg', alt='')
                    .portfolio-caption
                        h4 Startup Framework
                        p.text-muted Website Design
                .col-md-4.col-sm-6.portfolio-item
                    a.portfolio-link(href='#portfolioModal3', data-toggle='modal')
                        .portfolio-hover
                            .portfolio-hover-content
                              i.fa.ion-search.fa-4x
                        img.img-responsive(src='/images/header_bg.jpg', alt='')
                    .portfolio-caption
                        h4 Treehouse
                        p.text-muted Website Design
                .col-md-4.col-sm-6.portfolio-item
                    a.portfolio-link(href='#portfolioModal4', data-toggle='modal')
                        .portfolio-hover
                            .portfolio-hover-content
                              i.fa.ion-search.fa-4x
                        img.img-responsive(src='/images/header_bg.jpg', alt='')
                    .portfolio-caption
                        h4 Golden
                        p.text-muted Website Design
